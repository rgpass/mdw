<header class="navbar navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container">

			<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</a>

			<!-- Logo to replace this -->
			
			<%= link_to image_tag('http://tinyurl.com/k8k2dmb', class: 'brand'), root_path %>
			<div class="nav-collapse collapse">
				<ul class="nav">
					<li><%= link_to "Home", root_path %></li>
					<li><%= link_to "FAQ", faq_path %></li>
				</ul>
				<ul class="nav pull-right">
					<!-- signed_in? is defined in -->
					<!-- app/helpers/sessions_helper -->
					<% if signed_in? %>
						<li><%= link_to "Submit", new_post_path %></li>
						<li><%= link_to "View Profile", current_user %></li>
						<li><%= link_to "Edit Profile", edit_user_path(current_user) %></li>
						<li><%= link_to "Sign Out", signout_path, method: "delete" %></li>
					<% else %>
						<li><%= link_to "Sign In", signin_path %></li>
						<li><%= link_to "Sign Up", signup_path %></li>
					<% end %>
				</ul>


			</div>
		</div>
	</div>
</header>